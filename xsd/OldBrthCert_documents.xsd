<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="urn://mincomsvyaz/esia/OldBrthCert_documents/1.0.0" xmlns:types="urn://mincomsvyaz/esia/base_types/1.0.0" targetNamespace="urn://mincomsvyaz/esia/OldBrthCert_documents/1.0.0" elementFormDefault="qualified">
	<xs:import namespace="urn://mincomsvyaz/esia/base_types/1.0.0" schemaLocation="commons/base_types.xsd"/>
	<xs:element name="OldBrthCertDoc" type="tns:OldBrthCertDocType">
		<xs:annotation>
			<xs:documentation>Свидетельство о рождении, выданное в СССР</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="OldBrthCertDocType">
		<xs:annotation>
			<xs:documentation>Свидетельство о рождении, выданное в СССР</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="baseDoc" type="types:baseDocType">
				<xs:annotation>
					<xs:documentation>Серия, номер, дата свидетельства</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="issuedBy" type="xs:string">
				<xs:annotation>
					<xs:documentation>Кем выдано свидетельство</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="actNo" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Номер актовой записи</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="dateType">
		<xs:restriction base="xs:string">
			<xs:length value="10"/>
			<xs:pattern value="[0-3][0-9]\.[0-1][0-9]\.[0-9]{4}"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
