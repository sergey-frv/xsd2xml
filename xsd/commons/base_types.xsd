					<xs:documentation>Дата версии записи</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="childFullName" type="tns:childFullNameType">
				<xs:annotation>
					<xs:documentation>Сведения о ребенке</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="motherFullName" type="tns:fullNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Фамилия, имя, отчество матери</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fatherFullName" type="tns:fullNameType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Фамилия, имя, отчество отца</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="baseDoc" type="tns:baseSertType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Тип, серия, номер, дата свидетельства</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="registryOffice" type="tns:registryOffice" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Сведения об органе ЗАГС, которым произведена государственная регистрация рождения ребенка</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="childFullNameType">
		<xs:annotation>
			<xs:documentation>Сведения о ребенке</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="lastName" type="xs:string">
				<xs:annotation>
					<xs:documentation>Фамилия ребенка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="firstName" type="xs:string">
				<xs:annotation>
					<xs:documentation>Имя ребенка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="middleName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Отчество ребенка</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="birthDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>Дата рождения (календарная дата)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="gender">
				<xs:annotation>
					<xs:documentation>Пол</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="tns:string-1">
						<xs:enumeration value="1"/>
						<xs:enumeration value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="countryName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Наименование страны рождения</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="number-2">
		<xs:annotation>
			<xs:documentation>Число целое, содержащее до 2 разрядов</xs:documentation>
			<xs:documentation>Содержит только цифры</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string-1000">
		<xs:annotation>
			<xs:documentation>Произвольный текст длиной от 1 до 1000 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string-255">
		<xs:annotation>
			<xs:documentation>Произвольный текст длиной от 1 до 255 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="255"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string-100">
		<xs:annotation>
			<xs:documentation>Произвольный текст длиной от 1 до 100 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="100"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string-60">
